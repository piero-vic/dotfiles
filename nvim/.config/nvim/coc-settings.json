{
  "coc.preferences.formatOnSaveFiletypes": [
    "astro",
    "javascript",
    "javascriptreact",
    "json",
    "markdown"
  ],
  "emmet.includeLanguages": { "astro": "html" },
  "tailwindCSS.cssLanguages": ["css", "postcss", "sass", "scss"],
  "tailwindCSS.jsLanguages": ["javascript", "javascriptreact"],
  "tailwindCSS.htmlLanguages": ["astro", "html", "markdown", "svelte", "vue"],
  "tailwindCSS.headwind.runOnSave": false
}
